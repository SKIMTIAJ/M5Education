<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#fff"
    tools:context=".Activities.DataPostfile">


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/College_List_update"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView3"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Upload College List"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />



            </LinearLayout>

            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/School_List_update"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView8"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Upload School List"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />



            </LinearLayout>

            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/Way_List_Update"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView2"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Upload Way List"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />



            </LinearLayout>


            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/Scholarship_List_update"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add"/>

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView4"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Upload Scholarship"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp"/>


            </LinearLayout>


            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/Competetive_Examp_12"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView5"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Talent Examp after 12"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/simple_course_List_update"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView6"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Upload Simple Courses List"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/Youtube_Posting_page_image_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView7"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Post Youtube Video"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/AfterTen_Posting_page_image_btn"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView9"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Post Intermediate after Ten"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/PostRecycleItem_to_home_IMG"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView10"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Post Home Image"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />


            </LinearLayout>

            <LinearLayout
                android:layout_margin="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <ImageButton
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/toImageControlPage"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@android:drawable/ic_input_add" />

                <TextView
                    android:layout_weight="1"
                    android:layout_margin="5dp"
                    android:padding="5dp"
                    android:id="@+id/textView11"
                    android:layout_width="252dp"
                    android:layout_height="wrap_content"
                    android:text="Post Home Image"
                    android:textColor="@color/colorPrimary"
                    android:textSize="20sp" />


            </LinearLayout>


        </LinearLayout>


    </ScrollView>


</RelativeLayout>